<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/splash_graph"
    app:startDestination="@id/widgetsFragment">

    <fragment
        android:id="@+id/menuFragment"
        android:name="wee.digital.sample.ui.widgets.fragment.menu.MenuFragment"
        android:label="MenuFragment"
        tools:layout="@layout/menu" />
    <action
        android:id="@+id/action_global_menuFragment"
        app:destination="@id/menuFragment" />

    <fragment
        android:id="@+id/faceMeshFragment"
        android:name="wee.digital.sample.ui.widgets.fragment.mesh.FaceMeshFragment"
        android:label="FaceMeshFragment"
        tools:layout="@layout/face_mesh" />
    <action
        android:id="@+id/action_global_faceMeshFragment"
        app:destination="@id/faceMeshFragment" />

    <fragment
        android:id="@+id/faceRegFragment"
        android:name="wee.digital.sample.ui.widgets.fragment.reg.FaceRegFragment"
        android:label="FaceRegFragment"
        tools:layout="@layout/face_reg" />
    <action
        android:id="@+id/action_global_faceRegFragment"
        app:destination="@id/faceRegFragment" />

    <fragment
        android:id="@+id/camFragment"
        android:name="wee.digital.sample.ui.widgets.fragment.cam.CamFragment"
        android:label="CamFragment"
        tools:layout="@layout/cam" />
    <action
        android:id="@+id/action_global_camFragment"
        app:destination="@id/camFragment" />

    <fragment
        android:id="@+id/rsFragment"
        android:name="wee.digital.sample.ui.widgets.fragment.rs.RsFragment"
        android:label="RsFragment"
        tools:layout="@layout/rs" />
    <action
        android:id="@+id/action_global_rsFragment"
        app:destination="@id/rsFragment" />

</navigation>